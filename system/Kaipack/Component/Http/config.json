{
	"name": "http",

    "boot": {
        "name": "dispatcher",
        "class": "\\Kaipack\\Component\\Http\\Dispatcher"
    },

	"services": {

		"router": {
			"class": "\\Kaipack\\Component\\Http\\Router"
		},

		"request": {
			"class": "\\Symfony\\Component\\HttpFoundation\\Request",
            "factoryMethod": "createFromGlobals"
		},

		"response": {
			"class": "\\Symfony\\Component\\HttpFoundation\\Response"
		}

	}
}